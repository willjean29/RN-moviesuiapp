{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src/",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@screens/*": [
        "screens/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@theme/*": [
        "theme/*"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@api/*": [
        "api/*"
      ]
    }
  }
}